plot_co_data <- function(fn) {
fd <- read.csv(fn, header=FALSE)
names(fd) <- c('type', 'value')
plot(fd$value, col=fd$type)
legend('topleft', legend=names(table(fd$type)), col=c(1:3), pch=1)
return(fd)
}
